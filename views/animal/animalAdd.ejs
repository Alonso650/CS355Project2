<%- include("../includes/header.ejs", {title:"Add New Animal"}); %>
<h1>Add A New Animal</h1>
<form action="/animal/insert" method="get">
    Name: <input id="animal_name" name="animal_name" type="text" />
    Age: <input id="age" name="age" type="text" />
    Gender: <input id="gender" name="gender" type="text" />
    Date Entered: <input id="date_entered" name="date_entered" type="text" />
    <br/>
    <br/>
    Animal Located at:
    <br/>
    <select id="shelter_id" name="shelter_id" multiple="multiple">
        <% for(var i=0; shelter.length > i; i++) { %>
        <option value="<%= shelter[i].shelter_id %>"><%= shelter[i].shelter_name%></option>
        <% } %>
    </select>

    <script type="text/javascript">
        $(document).ready(function(){
            $('#shelter_id').multiselect();
        });
    </script>
<br/>
    <br />
    <input type="submit" />
</form>

<%- include("../includes/footer.ejs"); %>